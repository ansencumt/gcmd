{
	"name": "gcmd",
	"displayName": "gcmd",
	"description": "Launch a command prompt which contains the custom environment variables",
	"version": "1.0.0",
	"publisher": "Ansen",
	"engines": {
		"vscode": "^0.10.10"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.gcmd.run"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.gcmd.run",
			"title": "gcmd:run"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"vscode": "^0.11.0"
	}
}