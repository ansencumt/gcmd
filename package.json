{
	"name": "gcmd",
	"displayName": "gcmd",
	"description": "Launch a command prompt which contains the custom environment variables",
	"version": "1.0.0",
	"publisher": "ansen",
	"engines": {
		"vscode": "^0.10.10"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.gcmd.run"
	],
	"main": "./src/extension",
	"contributes": {
        "keybindings": [
			{
				"command": "extension.gcmd.run",
				"key": "ctrl+. ctrl+b",
				"when": "editorTextFocus && editorLangId == 'go'"
			}
		],
		"commands": [{
			"command": "extension.gcmd.run",
			"title": "gcmd:run"
		}]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"vscode": "^0.11.0"
	}
}